generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        String   @id @default(uuid())
  username  String?   @unique
  email     String   @unique
  password  String?
  provider  String   @default("credentials")
  createdAt DateTime @default(now())
}
